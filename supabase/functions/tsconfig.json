{
  "compilerOptions": {
    "lib": ["deno.window", "deno.ns", "deno.unstable"],
    "strict": true,
    "types": [],
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "checkJs": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}

